service <%= name %>
{
        port            = <%= port %>
        disable         = <%= disable %>
        socket_type     = <%= socket_type %>
        protocol        = <%= protocol %>
        wait            = <%= local_wait %>
        user            = <%= user %>
        group           = <%= group %>
        server          = <%= server %>
        bind            = <%= bind %>
<% if ! server_args.empty? -%>
        server_args     = <%= server_args %>
<% end -%>
<% if ! per_source.empty? -%>
        per_source      = <%= per_source %>
<% end -%>
<% if ! log_on_failure.empty? -%>
       log_on_failure  += <%= log_on_failure %>
<% end -%>
<% if ! cps.empty? -%>
        cps             = <%= cps %>
<% end -%>
<% if ! flags.empty? -%>
        flags           = <%= flags %>
<% end -%>
<% if ! service_type.empty? -%>
        type            = <%= service_type %>
<% end -%>
}

